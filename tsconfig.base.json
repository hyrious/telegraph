{
  "compilerOptions": {
    "noEmit": true,
    "target": "es2019",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strict": true,
    "noImplicitOverride": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "useUnknownInCatchVariables": false,
    "resolveJsonModule": true
  }
}
