{
  "include": ["src", "scripts"],
  "compilerOptions": {
    "strict": true,
    "module": "esnext",
    "target": "esnext",
    "lib": ["esnext"],
    "types": ["node"],
    "moduleResolution": "node",
    "skipLibCheck": true,
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "stripInternal": true
  }
}
